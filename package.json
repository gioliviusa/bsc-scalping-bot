{
  "name": "bsc-scalping-bot",
  "version": "1.0.0",
  "description": "Binance Smart Chain Scalping Bot",
  "main": "server.js",
  "scripts": {
  "start": "node server.js",
  "dev": "nodemon server.js",
  "debug": "BSC_DEBUG=true node server.js",
  "compile": "truffle compile",
  "deploy:testnet": "truffle migrate --network bscTestnet",
  "deploy:mainnet": "truffle migrate --network bscMainnet",
  "test": "truffle test",
  "frontend": "http-server . -p 8080 -c-1",
  "verify:testnet": "truffle run verify ProfitDistributor ScalpingBot --network bscTestnet",
  "verify:mainnet": "truffle run verify ProfitDistributor ScalpingBot --network bscMainnet"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.5",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.1",
    "helmet": "^7.0.0",
    "web3": "^1.8.0"
  },
  "devDependencies": {
    "@truffle/hdwallet-provider": "^2.1.6",
    "nodemon": "^2.0.22",
    "truffle": "^5.8.0"
  }
}